# Test array operations with scalars and ranges

# Array + Scalar operations  
arr1 = [10 20 30]
res1 = arr1 + 5
"Array + Scalar: -res1"

# Scalar + Array operations
res2 = 3 + arr1
"Scalar + Array: -res2"

# Array - Scalar
res3 = arr1 - 2
"Array - Scalar: -res3"

# Array * Scalar
res4 = arr1 * 2
"Array * Scalar: -res4"

# Prefix operations on arrays
arr2 = [1 -2 3]
res5 = -arr2
"Negation of array: -res5"

# Array with floats + scalar
arrFloat = [1.5 2.5 3.5]
res6 = arrFloat + 0.5
"Float Array + Scalar: -res6"

arrFloat2 = [1.25 2.75 3.25]
res6b = arrFloat2 + 2
"Float Array + Int Scalar: -res6b"

# Mixed expressions: Array + Range
arr3 = [1 2 3]
res7 = arr3 + (10:13)
"Array + Range: -res7"

# Range + Array
res8 = (5:8) + arr3
"Range + Array: -res8"

# Complex expression: Array + Range * Scalar
res9 = arr1 + (1:4) * 5
"Array + (Range * Scalar): -res9"

# Nested operations with arrays and ranges
res10 = (arr1 + 1) * (2:5)
"(Array + Scalar) * Range: -res10"

# In-place accumulation with range assignment
arr4 = [1 2 3]
arr4 = arr4 + (1:4)
"In-place Array + Range: -arr4"

# Mixed numeric operations
res11 = arrFloat / 2
"Float Array / Int Scalar: -res11"

res12 = arr1 / 2.0
"Int Array / Float Scalar: -res12"

res13 = arrFloat * 1.5
"Float Array * Float Scalar: -res13"

# Int array to float conversion
arrF = arr1 * 1.33
"Int Array * Float Scalar (conversion): -arrF"

# Nested expressions - infix operations with arrays
res14 = (arr1 + 5) * 2
"(Array + Scalar) * Scalar: -res14"

res15 = arr1 * (2 + 3)
"Array * (Scalar + Scalar): -res15"

res16 = (arrFloat / 2) + 1.0
"(Float Array / Scalar) + Scalar: -res16"

res17 = 3 * (arr1 - 1)
"Scalar * (Array - Scalar): -res17"

# Nested expressions - prefix operations with arrays
res18 = -(arr1 + 5)
"Negation of (Array + Scalar): -res18"

res19 = -(-arr2)
"Double negation of array: -res19"

res20 = -(arrFloat * 2.0)
"Negation of (Float Array * Scalar): -res20"

# Complex nested expressions combining multiple operations
res21 = (arr1 * 2) + 5
"(Int Array * Scalar) + Scalar: -res21"

res22 = -(arr1 - 5) + 10
"Negation of (Array - Scalar) + Scalar: -res22"

res23 = (arr1 + 1) * ((1:4) + 2)
"(Array + Scalar) * (Range + Scalar): -res23"
