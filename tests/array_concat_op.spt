# Test concatenation operator ⊕

# Integer concatenation
arr1 = [10 20]
arr2 = [30 40]
res1 = arr1 ⊕ arr2
"Int concat: -res1"

# Float concatenation
arrf1 = [1.5 2.5]
arrf2 = [3.0 4.25]
res2 = arrf1 ⊕ arrf2
"Float concat: -res2"

# Mixed int and float concatenation
mixedInt = [1 2]
mixedFloat = [3.5 4.5]
res3 = mixedInt ⊕ mixedFloat
"Int+Float concat: -res3"

# Different length arrays
arr3 = [100 200 300]
arr4 = [1]
res4 = arr3 ⊕ arr4
"Long + Short: -res4"

res5 = arr4 ⊕ arr3
"Short + Long: -res5"

# Empty array concatenation
arr5 = [1 2 3]
arr6 = []
res6 = arr5 ⊕ arr6
"Array + Empty: -res6"

res7 = arr6 ⊕ arr5
"Empty + Array: -res7"

# Building arrays with concatenation in loops
i = 0:5
built = []
built = built ⊕ [i]
"Built array: -built"
