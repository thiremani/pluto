arr = [10 20 30 40 50]
value = arr[2]
"Index: -value"

current = arr[1:4]
"RangeAssign: -current"

sum = 0
sum = sum + arr[0:5]
"RangeReduce: -sum"

res = arr[0:5] + 1
"RangeInfix: -res"

final = arr[0:5]
"RangeRoot: -final"

nested = arr[0:5] * 2 + 3
"NestedOp: -nested"

direct = [5 6 7 8][0:3]
"DirectLiteral: -direct"

idx = 0:5
viaIdent = arr[idx]
"IdentRange: -viaIdent"

pref = -arr[1:4]
"PrefixRange: -pref"

chain = [1]
chain = chain + [chain[idx]]
"LiteralIterChain: -chain"

callRoot = double(arr[0:5])
"CallRangeRoot: -callRoot"

callInfix = double(arr[0:5]) + idx
"CallRangeInfix: -callInfix"
