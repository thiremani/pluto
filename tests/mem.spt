# Basic copy semantics - strings
s1 = "hello"
s2 = s1
s1, s2
s2 = "world"
s1, s2

# Basic copy semantics - integer arrays
arr1 = [1 2 3]
arr2 = arr1
arr1, arr2
arr2 = [4 5 6]
arr1, arr2

# Basic copy semantics - float arrays
arr3 = [1.5 2.5]
arr4 = arr3
arr3, arr4
arr4 = [3.5 4.5]
arr3, arr4

# Basic copy semantics - string arrays
strs1 = ["a" "b"]
strs2 = strs1
strs1, strs2
strs2 = ["c" "d"]
strs1, strs2

# Swap optimization - strings (ownership transfer)
x = "first"
y = "second"
x, y
x, y = y, x
x, y

# String swap via function
p = "alpha"
q = "beta"
p, q
p, q = swapStr(p, q)
p, q

# String concatenation in function
base = "test"
result = createStr(base, "_suffix")
result

# Return string from function
s = returnStr()
s

# Multiple string literals from function
str1, str2, str3 = multiStr()
str1, str2, str3

# Multiple arrays from function
farr1, farr2 = multiArr()
farr1, farr2

# Heap string swap (strings from concatenation - tests ownership transfer)
h1 = "foo" ⊕ "bar"
h2 = "baz" ⊕ "qux"
h1, h2
h1, h2 = h2, h1
h1, h2

# Array swap (tests ownership transfer for arrays)
a1 = [10 20 30]
a2 = [40 50 60]
a1, a2
a1, a2 = a2, a1
a1, a2

# Duplicate RHS - string (a,b = a,a should copy second use to avoid double-free)
d1 = "dup" ⊕ "test"
d2 = "other"
d1, d2  # use before overwrite
d1, d2 = d1, d1
d1, d2

# Duplicate RHS - array (same pattern)
da1 = [100 200]
da2 = [300 400]
da1, da2  # use before overwrite
da1, da2 = da1, da1
da1, da2
