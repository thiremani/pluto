# Integer array concatenation
arr1 = [10 20]
arr2 = [30 40]
res1 = arr1 ⊕ arr2
"Int concat: -res1"

# Float array concatenation
arrf1 = [1.5 2.5]
arrf2 = [3.0 4.25]
res2 = arrf1 ⊕ arrf2
"Float concat: -res2"

# Mixed int and float concatenation
mixedInt = [1 2]
mixedFloat = [3.5 4.5]
res3 = mixedInt ⊕ mixedFloat
"Int⊕Float concat: -res3"
res4 = mixedFloat ⊕ mixedInt
"Float⊕Int concat: -res4"

# Empty array concatenation
empty = []
res5 = empty ⊕ arr1
"Empty⊕Int: -res5"

# Nested concatenation expressions
res6 = (arr1 ⊕ arr2) ⊕ [50 60]
"Nested concat left: -res6"
res7 = arr1 ⊕ (arr2 ⊕ [50 60])
"Nested concat right: -res7"
res8 = (arrf1 ⊕ mixedInt) ⊕ (mixedFloat ⊕ arrf2)
"Nested mixed concat: -res8"

# String concatenation (requires heap strings)
str1 = "hello".
str2 = "world".
res9 = str1 ⊕ str2
"String concat: -res9"

res10 = "". ⊕ str2
"Left empty concat: -res10"

str3 = "".
res11 = str1 ⊕ str3
"Right empty concat: -res11"

res12 = "". ⊕ str3
"Both empty str concat: -res12"
